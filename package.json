{
  "name": "htmxx",
  "version": "3.4.1",
  "description": "Full but minimalist responsive webapp framework with no client-side Javascript",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/stukennedy/htmxx.git"
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test jest --testTimeout=10000",
    "build": "npx tsc",
    "start": "node lib/index.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q lib/index.js\""
  },
  "keywords": [
    "htmx",
    "hyperscript",
    "mvc",
    "full-stack",
    "framework",
    "node"
  ],
  "author": "Stu Kennedy",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.20.0",
    "cheerio": "1.0.0-rc.12",
    "compression": "^1.7.4",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "express-ws": "^5.0.2",
    "mustache": "^4.2.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.14",
    "@types/express-ws": "^3.0.1",
    "@types/mustache": "^4.2.1",
    "@types/node": "^18.7.18",
    "chance": "^1.1.8",
    "concurrently": "^7.4.0",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.5",
    "jest": "^29.0.3",
    "nodemon": "^2.0.20",
    "supertest": "^6.2.4",
    "typescript": "^4.8.3",
    "uuid": "^9.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  }
}
